{
  "command": "jx",
  "url": "https://github.com/jenkins-x/jx/releases/download/v${VERSION}/jx-${env:lwpm_os}-${env:lwpm_architecture}.zip",
  "github": "jenkins-x/jx",
  "version": "2.1.33",
  "description": "Jenkins X provides automated CI+CD for Kubernetes with Preview Environments on Pull Requests using Tekton, Knative, Prow, Skaffold and Helm",
  "scripts": {
    "install": "_unzip  ,# Copy-item -r -force \"/\" \"\",# cp :LWPM_PKG_ROOT\\dist\\file \\dest\\path,# Start-Process -FilePath  -wait,# _cleanup \"\"\n",
    "download": "_mkdir $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}\n\nif(${env:lwpm_os} -ne \"windows\"){\n  $url = $url.trim('zip')\n  $url = \"${url}tar.gz\"\n}\n\ncurl -L $url `\n-o $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}/$filename\n",
    "uninstall": "_cleanup C:\\bin\\jx.exe\n"
  },
  "name": "jx",
  "platform": [
    {
      "architecture": "amd64",
      "os": "windows"
    },
    {
      "architecture": "amd64",
      "os": "darwin"
    },
    {
      "architecture": "amd64",
      "os": "linux"
    },
    {
      "architecture": "arm",
      "os": "linux"
    }
  ],
  "path": [
    "C:\\bin"
  ]
}
