{
  "url": "https://storage.googleapis.com/kubernetes-release/release/v${VERSION}/kubernetes-node-${env:lwpm_os}-${env:lwpm_architecture}.tar.gz",
  "scripts": {
    "dist": "_mkdir $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}\n\ncp -r $env:LWPM_PKG_ROOT/dist/kubernetes-node-${env:lwpm_os}-${env:lwpm_architecture}.tar.gz `\n      $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}/\n",
    "download": "_mkdir $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}\n\ncurl -L `\n  $url `\n  -o $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}/$filename\n"
  },
  "platform": [
    {
      "os": "linux",
      "architecture": "amd64"
    },
    {
      "os": "linux",
      "architecture": "arm64"
    },
    {
      "os": "linux",
      "architecture": "arm"
    },
    {
      "os": "windows",
      "architecture": "amd64"
    }
  ],
  "name": "kubernetes-node",
  "version": "1.18.0"
}
