{
  "platform": [
    {
      "architecture": "amd64",
      "os": "linux"
    },
    {
      "architecture": "arm64",
      "os": "linux"
    },
    {
      "architecture": "arm",
      "os": "linux"
    }
  ],
  "scripts": {
    "download": "mkdir -force $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture} | out-null\n\ncurl -L `\n  $url `\n  -o $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}/$filename\n",
    "dist": "_mkdir $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}\n\ncp -r $env:LWPM_PKG_ROOT/dist/kubernetes-server-${env:lwpm_os}-${env:lwpm_architecture}.tar.gz `\n      $env:LWPM_PKG_ROOT/dist/${env:lwpm_os}-${env:lwpm_architecture}/\n"
  },
  "name": "kubernetes-server",
  "version": "1.18.0",
  "url": "https://storage.googleapis.com/kubernetes-release/release/v${VERSION}/kubernetes-server-${env:lwpm_os}-${env:lwpm_architecture}.tar.gz"
}
